{
  "openapi": "3.0.1",
  "info": {
    "title": "api-usuarios",
    "version": "1.0"
  },
  "paths": {
    "/api/Login/register": {
      "post": {
        "tags": [
          "Login"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UsuarioCriacaoDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/UsuarioCriacaoDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/UsuarioCriacaoDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "UsuarioCriacaoDto": {
        "required": [
          "confirmaSenha",
          "email",
          "nome",
          "senha",
          "sobrenome",
          "usuario"
        ],
        "type": "object",
        "properties": {
          "usuario": {
            "minLength": 1,
            "type": "string"
          },
          "nome": {
            "minLength": 1,
            "type": "string"
          },
          "sobrenome": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string"
          },
          "dataCriacao": {
            "type": "string",
            "format": "date-time"
          },
          "dataAlteracao": {
            "type": "string",
            "format": "date-time"
          },
          "senha": {
            "minLength": 1,
            "type": "string"
          },
          "confirmaSenha": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
  }
}